{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "MkDocs Serve",
      "type": "debugpy",
      "request": "launch",
      "module": "mkdocs",
      "args": ["serve"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {},
      "justMyCode": false
    },
    {
      "name": "MkDocs Serve (Debug)",
      "type": "debugpy",
      "request": "launch",
      "module": "mkdocs",
      "args": ["serve", "--verbose"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {},
      "justMyCode": false,
      "stopOnEntry": false,
      "showReturnValue": true
    }
  ]
}
